#------------------------------------------------------------------------------#
# ~ irc_353
# ~ You get this when you join a channel, tells you the names of everyone there
sub
{
	my ($kernel,$server,$message) = @_[KERNEL, ARG0, ARG1];
	my($channel,$users) = split( /:/, $message );
	$channel =~ s/^.*#/#/gi;
	$channel =~ s/ +$//gi;
	foreach(  split(/ /,$users) )
	{
		s/.// if /^[\^\+\@\%]/;
		&user_set( $_, 'active', 1 );
	}
}
#______________________________________________________________________________#
